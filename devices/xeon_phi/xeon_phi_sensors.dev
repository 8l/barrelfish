    };

    /*
     * Protection Level: Ring 0
     * Visibility: Host / Coprocessor
     * Reset Dmain: CSR_RESET
     * Register Access: TRM
     */
    register ext_temp_settings4 rw addr(base, 0x10A0) "External Thermal Sensor Setting - Sensor #4" {
        value 32 "Value";
    };

    /*
     * Protection Level: Ring 0
